@*<div class="alert alert-info">
    This is a beta version of the audit.
</div>*@
<p style="font-style: italic" id="audit-summary">
    Audit last updated <b data-bind="text: lastUpdated"></b>. Analyzed <b data-bind="text: modelCount"></b> structures from the wwPDB CCD.
</p>
<div class="tabbable" style="margin-top: 20px">
    <ul class="nav nav-tabs">
        <li class="active"><a href="#wwpdbdict-summary" data-toggle="tab">Summary</a></li>
        <li><a href="#wwpdbdict-details" data-toggle="tab">Details</a></li>
    </ul>
    <div class="tab-content">
        <div class="tab-pane active" id="wwpdbdict-summary">
            <div id="wwpdbdict-summary-plot"></div>
        </div>
        <div class="tab-pane" id="wwpdbdict-details">
            <div class="form-inline">
                <div class="input-append pull-right" id="wwpdbdict-details-view-filter">
                    <input type="text" placeholder="Name Filter..." data-bind="disable: false, value: filterString, valueUpdate: 'input'" disabled>
                    <button class="btn" data-bind="disable: false, click: function() { filterString(''); }" disabled><i class="icon-remove"></i></button>
                </div>
                <a class="btn" href="@Url.Action("Download", new { id = "CsvWwPdbAudit" })"><i class="icon-download-alt"></i> <b>Download CSV Data</b></a>
            </div>
            <div id="wwpdbdict-details-view"></div>
        </div>
    </div>
</div>

<div id="wwpdbdict-overlay" style="background: white; position: absolute; left: 0; right: 0; top: 0; bottom: 0" data-bind="if: isBusy, visible: isBusy">
</div>